                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:textColor="@color/colorManatee"
                                android:text="Catatan Khusus"
                                android:textSize="16sp"
                                android:layout_weight="1"
                                android:textStyle="bold"
                                />
                            <TextView
                                android:id="@+id/txt_catatan"
                                android:textColor="@color/black_de"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content" />
                        </LinearLayout>
                    </LinearLayout>



                </LinearLayout>


            </android.support.v7.widget.CardView>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:layout_marginTop="10dp"
                android:orientation="horizontal">
                <Button
                    android:id="@+id/btn_verifikasi"
                    android:visibility="gone"
                    android:enabled="false"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/btn_disable_enable"
                    android:textColor="@color/colorWhite"
                    android:layout_weight="1"
                    android:layout_marginRight="10dp"
                    android:text="Verifikasi "/>
                <Button
                    android:id="@+id/btn_menilai"
                    android:layout_weight="1"
                    android:enabled="false"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/btn_disable_enable"
                    android:textColor="@color/colorWhite"
                    android:text="Menilai"/>
            </LinearLayout>


        </LinearLayout>
    </ScrollView>


</LinearLayout>
