<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorWhite"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:padding="7dp">


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="10dp"
            android:gravity="center"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"

                android:orientation="vertical"
                >
                <TextView
                    android:id="@+id/txt_udpdated"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/colorBlackFilkom"
                    android:layout_weight="1"
                    android:gravity="left"
                    android:layout_marginBottom="8dp"/>
            </LinearLayout>


        </LinearLayout>
        <android.support.v7.widget.CardView
            android:id="@+id/card_info_penilaian"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="10dp">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                >
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:background="@color/colorGrey"
                    android:padding="10dp">
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:text="Status Proses Penilaian"
                        android:layout_weight="0.3"
                        android:gravity="center_vertical"
                        android:textSize="18dp"
                        android:textColor="@color/colorBlackFilkom"
                        android:textStyle="bold"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:gravity="center_vertical"
                    android:layout_marginBottom="10dp">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="vertical">
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:text="Pembimbing "/>
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="10dp"
                            android:orientation="horizontal">
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:gravity="center"
                                android:visibility="gone"
                                android:id="@+id/ln_checkbox_pembimbing1"
                                android:layout_weight="1"
                                android:orientation="vertical">
                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="1"/>
                                <CheckBox
                                    android:id="@+id/checkbox_pembimbing1"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:button="@null"
                                    android:gravity="center"
                                    android:enabled="false"
                                    android:layout_weight="1"
                                    android:drawableTop="@drawable/checkbox_select_unselect"
                                    android:text="Prosess"/>
                            </LinearLayout>
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:gravity="center"
                                android:id="@+id/ln_checkbox_pembimbing2"
                                android:layout_weight="1"
                                android:visibility="gone"
                                android:orientation="vertical">
                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="2"/>
                                <CheckBox
                                    android:id="@+id/checkbox_pembimbing2"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:button="@null"
                                    android:gravity="center"
                                    android:enabled="false"
                                    android:layout_weight="1"
                                    android:drawableTop="@drawable/checkbox_select_unselect"
                                    android:text="Prosess"/>
                            </LinearLayout>

                        </LinearLayout>

                    </LinearLayout>
                    <View
                        android:layout_width="1dp"
                        android:layout_height="80dp"
                        android:alpha="0.1"
                        android:background="@color/colorBlackFilkom"></View>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="vertical">
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:layout_marginBottom="10dp"
                            android:text="Penguji"/>
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:gravity="center"
                                android:layout_weight="1"
                                android:orientation="vertical">
                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="1"/>
                                <CheckBox
                                    android:id="@+id/checkbox_penguji1"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:button="@null"
                                    android:gravity="center"
                                    android:enabled="false"
                                    android:layout_weight="1"
                                    android:drawableTop="@drawable/checkbox_select_unselect"
                                    android:text="Prosess"/>
                            </LinearLayout>
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:gravity="center"
                                android:layout_weight="1"
                                android:orientation="vertical">
                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="2"/>
                                <CheckBox
                                    android:id="@+id/checkbox_penguji2"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:button="@null"
                                    android:gravity="center"
                                    android:enabled="false"
                                    android:layout_weight="2"
                                    android:drawableTop="@drawable/checkbox_select_unselect"
                                    android:text="Prosess"/>
                            </LinearLayout>

                        </LinearLayout>

                    </LinearLayout>

                </LinearLayout>
            </LinearLayout>


        </android.support.v7.widget.CardView>
        <android.support.v7.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="10dp">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:background="@color/colorGrey"
                    android:padding="10dp">
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:text="Nila Skripsi"
                        android:layout_weight="1"
                        android:textSize="18dp"
                        android:textStyle="bold"
                        android:gravity="center_vertical"
                        android:textColor="@color/colorBlackFilkom"
                        />
                    <TextView
                        android:id="@+id/txt_nilai_akhir_skripsi_huruf"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:textColor="@color/colorBlackFilkom"
                        android:gravity="right"
                        android:textSize="20dp"
                        android:textStyle="bold|italic"
                        />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:padding="10dp"

                    android:orientation="vertical">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="@color/colorQuarts"
                        android:text="Kalkulasi Nilai"
                        android:textSize="16sp"
                        android:textStyle="bold"
                        android:layout_marginBottom="5dp"/>
                    <View
                        android:layout_width="100dp"
                        android:layout_height="1dp"
                        android:background="@color/colorQuarts"></View>
                </LinearLayout>


                <HorizontalScrollView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:fillViewport="true">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">
                        <TableLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:padding="10dp">

                            <TableRow
                                android:layout_height="wrap_content"
                                android:layout_width="match_parent"
                                >
                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text="Keterangan"
                                    android:textColor="@color/colorQuarts"
                                    android:layout_span="1"
                                    android:textStyle="bold"
                                    android:layout_weight="1" />

                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text="Rata-rata"
                                    android:textColor="@color/colorQuarts"
                                    android:layout_span="1"
                                    android:gravity="center"
                                    android:textStyle="bold"

                                    android:layout_marginRight="8dp"
                                    android:layout_weight="1" />
                                <TextView
                                    android:layout_width="match_parent" android:layout_height="wrap_content"
                                    android:text="Bobot"
                                    android:textColor="@color/colorQuarts"
                                    android:layout_span="1"
                                    android:gravity="center"
                                    android:textStyle="bold"

                                    android:layout_marginRight="8dp"

                                    android:layout_weight="1" />
                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text="Sub total"
                                    android:layout_span="1"
                                    android:textStyle="bold"
                                    android:textColor="@color/colorQuarts"
                                    android:gravity="center"
                                    android:layout_weight="1" />

                            </TableRow>
                            <TableRow
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="10dp">
                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text="Seminar Hasil"
                                    android:layout_span="1"
                                    android:textColor="@color/colorQuarts"
                                    android:layout_marginRight="8dp"
                                    android:layout_weight="1" />

                                <TextView
                                    android:id="@+id/txt_semhas"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text="0"
                                    android:layout_span="1"
                                    android:gravity="center"
                                    android:layout_weight="1" />
                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text="5%"
                                    android:layout_span="1"
                                    android:gravity="center"

                                    android:layout_weight="1" />
                                <TextView
                                    android:id="@+id/txt_sub_total_semhas"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text="0"
                                    android:layout_span="1"
                                    android:gravity="center"

                                    android:layout_weight="1" />
                            </TableRow>
                            <TableRow
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="10dp">
                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text="Publikasi"
                                    android:layout_span="1"
                                    android:layout_marginRight="8dp"
                                    android:textColor="@color/colorQuarts"

                                    android:layout_weight="1" />

                                <TextView
                                    android:id="@+id/txt_nilai_publikasi"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text="0"
                                    android:layout_span="1"
                                    android:gravity="center"

                                    android:layout_weight="1" />
                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text="10%"
                                    android:layout_span="1"
                                    android:gravity="center"

                                    android:layout_weight="1" />
                                <TextView
                                    android:id="@+id/txt_sub_total_publikasi"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text="0"
                                    android:layout_span="1"
                                    android:gravity="center"

                                    android:layout_weight="1" />
                            </TableRow>
                            <TableRow
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="10dp">
                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text="Ujian Skripsi\n(pembimbing)"
                                    android:layout_span="1"
                                    android:layout_marginRight="8dp"
                                    android:textColor="@color/colorQuarts"

                                    android:layout_weight="1" />

                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:text="0"
                                    android:layout_span="1"
                                    android:gravity="center"
                                    android:id="@+id/txt_ujian_skripsi1"
                                    android:layout_weight="1" />
                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:text="55%"
                                    android:layout_span="1"
                                    android:gravity="center"

                                    android:layout_weight="1" />
                                <TextView
                                    android:id="@+id/txt_sub_total_nilai_ujian1"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:text="0"
                                    android:layout_span="1"
                                    android:gravity="center"

                                    android:layout_weight="1" />

                            </TableRow>
                            <TableRow
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="10dp">
                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text="Ujian Skripsi \n(penguji)"
                                    android:layout_span="1"
                                    android:layout_marginRight="8dp"
                                    android:textColor="@color/colorQuarts"

                                    android:layout_weight="1" />

                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:text="0"
                                    android:layout_span="1"
                                    android:gravity="center"
                                    android:id="@+id/txt_ujian_skripsi2"

                                    android:layout_weight="1" />
                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:text="30%"
                                    android:layout_span="1"
                                    android:gravity="center"

                                    android:layout_weight="1" />
                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:text="0"
                                    android:layout_span="1"
                                    android:gravity="center"
                                    android:id="@+id/txt_sub_total_nilai_ujian2"

                                    android:layout_weight="1" />

                            </TableRow>
                            <TableRow
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="10dp">
                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text="Nilai Akhir"
                                    android:layout_span="3"
                                    android:gravity="center"
                                    android:textColor="@color/colorQuarts"
                                    android:layout_weight="0.18" />


                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text="0"
                                    android:id="@+id/txt_nilai_akhir_total"
                                    android:layout_span="1"
                                    android:gravity="center"
                                    android:layout_weight="1" />

                            </TableRow>
                        </TableLayout>

                    </LinearLayout>


                </HorizontalScrollView>
                <TextView
                    android:id="@+id/more_nilai_skripsi"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:padding="12dp"
                    android:gravity="center"
                    android:text="Lihat Selengkapnya"
                    android:background="@color/colorWhiteSmoke"
                    android:textColor="@color/colorQuarts"
                    android:textSize="13dp" />
            </LinearLayout>


        </android.support.v7.widget.CardView>
        <android.support.v7.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="10dp">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:background="@color/colorGrey"
                    android:padding="10dp">
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:text="Nila Kualitas Penulisan Skripsi"
                        android:gravity="center_vertical"
                        android:textSize="18dp"
                        android:layout_weight="1"
                        android:layout_marginRight="15dp"
                        android:textColor="@color/colorBlackFilkom"
                        android:textStyle="bold"/>
                    <TextView
                        android:id="@+id/txt_nilai_akhir_penulisan_huruf"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="@color/colorBlackFilkom"
                        android:gravity="right"
                        android:textSize="20dp"
                        android:textStyle="bold|italic"
                        />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:padding="10dp"

                    android:orientation="vertical">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="@color/colorQuarts"
                        android:text="Kalkulasi Nilai"
                        android:textSize="16sp"
                        android:textStyle="bold"
                        android:layout_marginBottom="5dp"/>
                    <View
                        android:layout_width="100dp"
                        android:layout_height="1dp"
                        android:background="@color/colorQuarts"></View>
                </LinearLayout>


                <HorizontalScrollView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:fillViewport="true">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">
                        <TableLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:padding="10dp">

                            <TableRow
                                android:layout_height="wrap_content"
                                android:layout_width="match_parent"
                                >
                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text="Keterangan"
                                    android:textColor="@color/colorQuarts"
                                    android:layout_span="1"
                                    android:textStyle="bold"
                                    android:layout_weight="1" />

                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text="Rata-rata"
                                    android:textColor="@color/colorQuarts"
                                    android:layout_span="1"
                                    android:gravity="center"
                                    android:textStyle="bold"

                                    android:layout_marginRight="8dp"
                                    android:layout_weight="1" />
                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text="Bobot"
                                    android:textColor="@color/colorQuarts"
                                    android:layout_span="1"
                                    android:gravity="center"
                                    android:textStyle="bold"

                                    android:layout_marginRight="8dp"

                                    android:layout_weight="1" />
                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text="Sub total"
                                    android:layout_span="1"
                                    android:textStyle="bold"
                                    android:textColor="@color/colorQuarts"
                                    android:gravity="center"
                                    android:layout_weight="1" />

                            </TableRow>

                            <TableRow
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="10dp">
                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text="Kualitas Penulisan \n(pembimbing)"
                                    android:layout_span="1"
                                    android:layout_marginRight="8dp"
                                    android:textColor="@color/colorQuarts"

                                    android:layout_weight="1" />

                                <TextView
                                    android:id="@+id/txt_nilai_kualitas_pembimbing"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:text="0"
                                    android:layout_span="1"
                                    android:gravity="center"

                                    android:layout_weight="1" />
                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:text="60%"
                                    android:layout_span="1"
                                    android:gravity="center"

                                    android:layout_weight="1" />
                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:text="0"
                                    android:layout_span="1"
                                    android:gravity="center"
                                    android:id="@+id/txt_sub_total_nilai_kualitas"

                                    android:layout_weight="1" />

                            </TableRow>
                            <TableRow
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="10dp">
                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text="Kualitas Penulisan\n(penguji)"
                                    android:layout_span="1"
                                    android:layout_marginRight="8dp"
                                    android:textColor="@color/colorQuarts"

                                    android:layout_weight="1" />

                                <TextView
                                    android:id="@+id/txt_nilai_kualitas_penguji1"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:text="0"
                                    android:layout_span="1"
                                    android:gravity="center"

                                    android:layout_weight="1" />
                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:text="40%"
                                    android:layout_span="1"
                                    android:gravity="center"

                                    android:layout_weight="1" />
                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:text="0"
                                    android:layout_span="1"
                                    android:gravity="center"
                                    android:id="@+id/txt_sub_total_nilai_kualitas1"

                                    android:layout_weight="1" />

                            </TableRow>
                            <TableRow
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="10dp">
                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text="Nilai Akhir"
                                    android:layout_span="3"
                                    android:gravity="center"
                                    android:textColor="@color/colorQuarts"
                                    android:layout_weight="0.18" />


                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text="0"
                                    android:id="@+id/txt_nilai_akhir_penulisan"
                                    android:layout_span="1"
                                    android:gravity="center"
                                    android:layout_weight="1" />

                            </TableRow>
                        </TableLayout>

                    </LinearLayout>


                </HorizontalScrollView>

                <TextView
                    android:id="@+id/more_nilai_kualitas"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:padding="12dp"
                    android:gravity="center"
                    android:text="Lihat Selengkapnya"
                    android:background="@color/colorWhiteSmoke"
                    android:textColor="@color/colorQuarts"
                    android:textSize="13dp" />
            </LinearLayout>


        </android.support.v7.widget.CardView>
        <android.support.v7.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="10dp">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:background="@color/colorGrey"
                    android:padding="10dp">
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:text="Berita Acara"
                        android:layout_weight="0.3"
                        android:textSize="18dp"
                        android:gravity="center_vertical"
                        android:textColor="@color/colorBlackFilkom"
                        android:textStyle="bold"/>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:padding="10dp"

                    android:orientation="vertical">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:layout_marginBottom="5dp">
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:textColor="@color/colorManatee"
                            android:text="Nilai"
                            android:textSize="16sp"
                            android:layout_weight="1"
                            />
                        <TextView
                            android:id="@+id/txt_nilai_akhir"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:textColor="@color/colorBlackFilkom"
                            android:text="A(85)"
                            android:textSize="16sp"
                            android:layout_weight="1"

                            />
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:layout_marginBottom="5dp">
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:textColor="@color/colorManatee"
                            android:text="Status Kelulusan Mahasisiwa"
                            android:textSize="16sp"
                            android:layout_weight="1"
                            />

                        <Spinner
                            android:id="@+id/spiner_status"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:entries="@array/predikat"
                            style="@style/Widget.AppCompat.Spinner.Underlined"
                            android:layout_weight="1"
                            android:textSize="14sp" />
                    </LinearLayout>


                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:layout_marginBottom="5dp">
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:textColor="@color/colorQuarts"
                            android:text="Catatan Khusus"
                            android:textSize="16sp"
                            android:layout_weight="1"
                            android:textStyle="bold"
                            />
                        <EditText
                            android:id="@+id/txt_catatan"
                            android:gravity="top"
                            android:background="@drawable/border"
                            android:layout_width="match_parent"
                            android:layout_height="150dp"
                            android:paddingTop="5dp"
                            android:paddingLeft="5dp"
                            android:paddingRight="5dp"
                            android:inputType="textMultiLine"/>
                    </LinearLayout>
                </LinearLayout>



            </LinearLayout>


        </android.support.v7.widget.CardView>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:orientation="horizontal">

            <Button
                android:id="@+id/btn_submit"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/btn_disable_enable"
                android:textColor="@color/colorWhite"
                android:layout_weight="1"
                android:text="Submit"/>
        </LinearLayout>

    </LinearLayout>

</LinearLayout>